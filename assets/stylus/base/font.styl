@font-face {
  font-family:'Messina Sans';
  font-display: swap;
  src: url('~/assets/fonts/messina_sans_light.eot');
  src: url('~/assets/fonts/messina_sans_light.eot?#iefix') format('embedded-opentype'),
  url('~/assets/fonts/messina_sans_light.woff2') format('woff2'),
  url('~/assets/fonts/messina_sans_light.woff') format('woff'),
  url('~/assets/fonts/messina_sans_light.svg#messina_sans_light') format('svg');
  font-weight: 300;
  font-style: normal;
  font-stretch: normal;
  unicode-range: U+0020-00FE;
}

